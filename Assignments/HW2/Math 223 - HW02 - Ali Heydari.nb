    RowBox[{
     RowBox[{"x", " ", 
      RowBox[{"y", "''"}]}], " ", "+", " ", "y"}], "=", " ", "0"}], 
   TraditionalForm]],ExpressionUUID->"e4fd86ad-714f-444a-a212-1eaac2726d80"]
