Cell[TextData[{
 "Show that ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         SubsuperscriptBox["\[Integral]", "0", "1"], 
         FractionBox[
          RowBox[{
            SuperscriptBox[
             StyleBox["e", "TI"], 
             StyleBox["x", "TI"]], "-", 
            SuperscriptBox[
             StyleBox["e", "TI"], 
             RowBox[{
               StyleBox["x", "TI"], 
               StyleBox["t", "TI"]}]]}], 
          RowBox[{"1", "-", 
            StyleBox["t", "TI"]}]], 
         StyleBox["d", "TI"], 
         StyleBox["t", "TI"], "\[Tilde]", 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          StyleBox["x", "TI"]], "log", 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], "+", 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          StyleBox["x", "TI"]], "\[Gamma]", "+", "\[CenterEllipsis]", ","}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "\\int_0^1 \\frac{e^x - e^{xt}}{1-t} dt \\sim e^x \\log(x) + e^x \\gamma \
+ \\cdots ,", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "54ebd178-cbb0-4857-b11d-430bfcc4c162"],
 " as ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         StyleBox["x", "TI"], "\[Rule]", "\[Infinity]"}], TraditionalForm], 
     "errors" -> {}, "input" -> "x \\rightarrow \\infty", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "56ba7bfd-e656-43c6-ace5-4ca851002792"],
 " with ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association[
    "boxes" -> FormBox["\[Gamma]", TraditionalForm], "errors" -> {}, "input" -> 
     "\\gamma", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "ec2871f4-572a-4390-936f-dcd7ba6e2939"],
 " being the Euler-Mascheroni constant, defined according to ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[Association["boxes" -> FormBox[
        RowBox[{
          SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
          RowBox[{"(", 
            RowBox[{
              FractionBox["1", 
               RowBox[{
                 StyleBox["u", "TI"], "+", "1"}]], "-", 
              SuperscriptBox[
               StyleBox["e", "TI"], 
               RowBox[{"-", 
                 StyleBox["u", "TI"]}]]}], ")"}], 
          FractionBox[
           RowBox[{
             StyleBox["d", "TI"], 
             StyleBox["u", "TI"]}], 
           StyleBox["u", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
      "\\int_0^\\infty (\\frac{1}{u+1} - e^{-u}) \\frac{du}{u}", "state" -> 
      "Boxes"],
     "TeXAssistantTemplate"], "."}], TraditionalForm]],ExpressionUUID->
  "3cc71427-8bc4-4e85-9fd7-440dabaea944"]
}], "Text",
