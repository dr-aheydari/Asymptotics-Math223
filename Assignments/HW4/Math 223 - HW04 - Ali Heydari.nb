             RowBox[{
              SuperscriptBox["c", "\[Prime]",
               MultilineFunction->None], "[", "x", "]"}]}]}], ")"}], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "1"}], "+", 
            RowBox[{"2", " ", 
             SuperscriptBox["x", 
              RowBox[{"3", "/", "2"}]], " ", 
             RowBox[{
              SuperscriptBox["c", "\[Prime]\[Prime]",
               MultilineFunction->None], "[", "x", "]"}]}]}], ")"}]}], "+", 
         RowBox[{"4", " ", 
          SuperscriptBox["x", 
           RowBox[{"5", "/", "2"}]], " ", 
