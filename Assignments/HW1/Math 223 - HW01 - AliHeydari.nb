  RowBox[{"{", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"y", "[", "x", "]"}], "\[Rule]", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{"-", "x"}], "+", 
          FractionBox["1", "\[Epsilon]"], "-", 
          FractionBox["x", "\[Epsilon]"]}]], " ", 
        RowBox[{"(", 
