Cell[TextData[{
 "\t\[Epsilon]y\[CloseCurlyQuote]\[CloseCurlyQuote] + (1+\[Epsilon])y\
\[CloseCurlyQuote] + y = 0\n\ty(0) = 0,\n\ty(1) = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"-", "1"}]], TraditionalForm]],ExpressionUUID->
  "819b0eee-f080-493c-aacc-442dc875633a"],
 "\n\nRead the documentation to learn how to plot the solution for different \
values 0 < \[Epsilon] <= 1. Comment on how the solution changes with \
\[Epsilon]."
}], "Text",
 CellChangeTimes->{{3.820683056348654*^9, 3.820683094787992*^9}, {
  3.820683181963314*^9, 3.820683284496016*^9}, {3.820683552567935*^9, 
