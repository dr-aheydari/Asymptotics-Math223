  3.820683575320014*^9}},ExpressionUUID->"f2e34ee4-1f88-402c-ac34-\
9b17e17cf7ec"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"ode", " ", "=", " ", 
  RowBox[{
   RowBox[{
    RowBox[{"\[Epsilon]", " ", "*", " ", 
     RowBox[{
      RowBox[{"y", "''"}], "[", "x", "]"}]}], "+", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", " ", "+", " ", "\[Epsilon]"}], ")"}], " ", "*", " ", 
     RowBox[{
      RowBox[{"y", "'"}], "[", "x", "]"}]}], " ", "+", " ", 
    RowBox[{"y", "[", "x", "]"}]}], "\[Equal]", 
   "0"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"y0", " ", "=", " ", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"y1", " ", "=", " ", 
   RowBox[{"\[ExponentialE]", "^", 
    RowBox[{"-", "1"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"solution", " ", "=", " ", 
  RowBox[{"DSolve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"ode", ",", 
      RowBox[{
       RowBox[{"y", "[", "0", "]"}], " ", "\[Equal]", " ", "y0"}], ",", 
      RowBox[{
       RowBox[{"y", "[", "1", "]"}], "\[Equal]", "y1"}]}], " ", "}"}], ",", 
    RowBox[{"y", "[", "x", "]"}], ",", "x"}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.820683289808936*^9, 3.820683308083149*^9}, {
  3.820683421006339*^9, 3.820683477688302*^9}, {3.82068350784054*^9, 
  3.820683522880155*^9}, {3.820683740684237*^9, 3.820683808408305*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"c36f266a-a6eb-4e0c-a40c-4c3189ee6587"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"y", "[", "x", "]"}], "+", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"1", "+", "\[Epsilon]"}], ")"}], " ", 
    RowBox[{
     SuperscriptBox["y", "\[Prime]",
      MultilineFunction->None], "[", "x", "]"}]}], "+", 
   RowBox[{"\[Epsilon]", " ", 
    RowBox[{
     SuperscriptBox["y", "\[Prime]\[Prime]",
      MultilineFunction->None], "[", "x", "]"}]}]}], "\[Equal]", 
  "0"}]], "Output",
