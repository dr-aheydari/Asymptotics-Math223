Cell[TextData[{
 "so now that we have found the asymptotic matching, we can write the uniform \
solution as: \n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         StyleBox["y", "TI"], "\[LongEqual]", 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          RowBox[{"-", 
            StyleBox["X", "TI"]}]], 
         RowBox[{"(", 
           RowBox[{"-1", "+", "2", 
             SuperscriptBox[
              StyleBox["e", "TI"], 
              StyleBox["X", "TI"]]}], ")"}], "+", 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          RowBox[{"-", 
            StyleBox["x", "TI"]}]], "+", 
         FractionBox["1", "\[Epsilon]"], "-", "2"}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "y = e^{-X}(-1 + 2e^{X}) + e^{-x} + \\frac{1}{\\epsilon} - 2", "state" -> 
     "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "8a1b888b-ac8e-420e-8f92-bbe8461c9802"],
 "\n\nreplacing ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       StyleBox["X", "TI"], TraditionalForm], "errors" -> {}, "input" -> "X", 
     "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "efd67f36-d920-4623-af7c-2a87aed0c37d"],
 " with ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       FractionBox[
        StyleBox["x", "TI"], "\[Epsilon]"], TraditionalForm], "errors" -> {}, 
     "input" -> "\\frac{x}{\\epsilon}", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "13a3b523-9eda-4300-a578-7b72b0099645"],
 " we get:\n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         StyleBox["y", "TI"], "\[LongEqual]", 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          RowBox[{"-", 
            FractionBox[
             StyleBox["x", "TI"], "\[Epsilon]"]}]], 
         RowBox[{"(", 
           RowBox[{"-1", "+", "2", 
             SuperscriptBox[
              StyleBox["e", "TI"], 
              FractionBox[
               StyleBox["x", "TI"], "\[Epsilon]"]]}], ")"}], "+", 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          RowBox[{"-", 
            StyleBox["x", "TI"]}]], "+", 
         FractionBox["1", "\[Epsilon]"], "-", "2", "\[LongEqual]", "-", 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          RowBox[{"-", 
            FractionBox[
             StyleBox["x", "TI"], "\[Epsilon]"]}]], "+", "2", "+", 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          RowBox[{"-", 
            StyleBox["x", "TI"]}]], "+", 
         FractionBox["1", "\[Epsilon]"], "-", "2"}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "y = e^{-\\frac{x}{\\epsilon}}(-1 + 2e^{\\frac{x}{\\epsilon}}) + e^{-x} \
+ \\frac{1}{\\epsilon} - 2 = -e^{-\\frac{x}{\\epsilon}}+ 2 + e^{-x} + \
\\frac{1}{\\epsilon} - 2", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "863918f1-b66e-4e8a-93f8-a22b7296cd44"]
}], "Text",
