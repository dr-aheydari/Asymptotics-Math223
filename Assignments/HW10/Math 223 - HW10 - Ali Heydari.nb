Cell[TextData[{
 "so now that we have found the asymptotic matching, we can write the uniform \
solution as: \n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         StyleBox["y", "TI"], "\[LongEqual]", "1", "+", 
         FractionBox["1", 
          RowBox[{
            StyleBox["a", "TI"], "(", "0", ")"}]], "-", 
         FractionBox[
          SuperscriptBox[
           StyleBox["e", "TI"], 
           RowBox[{"-", 
             StyleBox["a", "TI"], 
             RowBox[{"(", "0", ")"}], 
             StyleBox["X", "TI"]}]], 
          RowBox[{
            StyleBox["a", "TI"], "(", "0", ")"}]], "+", 
         FractionBox["1", 
          SuperscriptBox[
           StyleBox["e", "TI"], 
           RowBox[{"-", 
             SubsuperscriptBox["\[Integral]", "0", "1"], 
             FractionBox[
              RowBox[{
                StyleBox["b", "TI"], "(", 
                StyleBox["t", "TI"], ")"}], 
              RowBox[{
                StyleBox["a", "TI"], "(", 
                StyleBox["t", "TI"], ")"}]]}]]], 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          RowBox[{"-", 
            SubsuperscriptBox["\[Integral]", 
             StyleBox["x", "TI"], "1"], 
            FractionBox[
             RowBox[{
               StyleBox["b", "TI"], "(", 
               StyleBox["t", "TI"], ")"}], 
             RowBox[{
               StyleBox["a", "TI"], "(", 
               StyleBox["t", "TI"], ")"}]], 
            StyleBox["d", "TI"], 
            StyleBox["t", "TI"]}]], "-", "1"}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "y =  1 + \\frac{1}{a(0)} -\\frac{e^{-a(0)X}}{a(0)} +  \
\\frac{1}{e^{-\\int_0^1 \\frac{b(t)}{a(t)}}}e^{-\\int_{x}^1 \
\\frac{b(t)}{a(t)} dt }- 1", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],
  FontSize->24,ExpressionUUID->"8a1b888b-ac8e-420e-8f92-bbe8461c9802"],
 "\n\nreplacing ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       StyleBox["X", "TI"], TraditionalForm], "errors" -> {}, "input" -> "X", 
     "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "efd67f36-d920-4623-af7c-2a87aed0c37d"],
 " with ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       FractionBox[
        StyleBox["x", "TI"], "\[Epsilon]"], TraditionalForm], "errors" -> {}, 
     "input" -> "\\frac{x}{\\epsilon}", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "13a3b523-9eda-4300-a578-7b72b0099645"],
