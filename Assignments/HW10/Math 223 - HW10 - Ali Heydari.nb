       SubscriptBox[
        StyleBox["y", "TI"], 
        RowBox[{
          StyleBox["i", "TI"], 
          StyleBox["n", "TI"]}]], TraditionalForm], "errors" -> {}, "input" -> 
     "y_{in}", "state" -> "Boxes"],
