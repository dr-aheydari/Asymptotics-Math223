Cell[TextData[{
 "Use boundary-layer methods to find an approximate solution to the \
initial-value problem\n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{"\[Epsilon]", 
         SuperscriptBox[
          StyleBox["y", "TI"], "\[Prime]\[Prime]"], "+", 
         StyleBox["a", "TI"], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], 
         SuperscriptBox[
          StyleBox["y", "TI"], "\[Prime]"], "+", 
         StyleBox["b", "TI"], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], 
         StyleBox["y", "TI"], "\[LongEqual]", "0", ","}], TraditionalForm], 
     "errors" -> {}, "input" -> "\\epsilon y'' + a(x) y' + b(x) y = 0,", 
     "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "493e17c2-f9fa-4994-95b5-f1ec22aadfb5"],
 "\t\t",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         StyleBox["x", "TI"], "\[GreaterEqual]", "0", ","}], TraditionalForm],
      "errors" -> {}, "input" -> "x \\geq 0,", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "8e489ec3-352b-4fe9-9498-28b63d784606"],
 "\t",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         StyleBox["a", "TI"], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], ">", "0", ","}], TraditionalForm], 
     "errors" -> {}, "input" -> "a(x) > 0,", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "8c9de7c1-56d3-4960-a120-a213531eaee9"],
 "\t\t",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{"0", "<", "\[Epsilon]", "\[LessLess]", "1"}], TraditionalForm],
      "errors" -> {}, "input" -> "0<\\epsilon \\ll 1", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "ba2f9ace-aebb-4a9a-af75-36ff90134f22"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         StyleBox["y", "TI"], 
         RowBox[{"(", "0", ")"}], "\[LongEqual]", "1"}], TraditionalForm], 
     "errors" -> {}, "input" -> "y(0) = 1", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "cb3fafc8-dfbe-4311-bbe8-2268c9a3c016"],
 ", \t",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          StyleBox["y", "TI"], "\[Prime]"], 
         RowBox[{"(", "0", ")"}], "\[LongEqual]", "1"}], TraditionalForm], 
     "errors" -> {}, "input" -> "y'(0) = 1", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "522b2b4b-3d02-4ca3-836f-c0b61cf0a822"]
}], "Text",
 CellChangeTimes->{{3.8194771895995965`*^9, 3.8194771998661075`*^9}, {
