Cell[TextData[{
 "Use second-order perturbation theory to find approximations to the roots of \
",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          StyleBox["x", "TI"], "2"], "+", 
         StyleBox["x", "TI"], "+", "6", "\[Epsilon]", "\[LongEqual]", "0"}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "x^2 + x + 6\\epsilon = 0", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "669c7730-82e2-4df5-b2ff-d62145e999c7"],
 "."
