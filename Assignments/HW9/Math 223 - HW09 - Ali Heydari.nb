Cell[TextData[{
 "Use second-order perturbation theory to find approximations to the roots of \
",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          StyleBox["x", "TI"], "2"], "+", 
         StyleBox["x", "TI"], "+", "6", "\[Epsilon]", "\[LongEqual]", "0"}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "x^2 + x + 6\\epsilon = 0", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "669c7730-82e2-4df5-b2ff-d62145e999c7"],
 "."
}], "Text",
 CellChangeTimes->{{3.823272161170442*^9, 3.8232721651934137`*^9}, {
  3.823272397584096*^9, 3.823272557020988*^9}, {3.823272644521634*^9, 
  3.8232726452007303`*^9}, {3.8268433844189157`*^9, 
  3.826843430084824*^9}},ExpressionUUID->"d9d81440-dd2a-4915-ae1b-\
9469cd2d3476"],

Cell[TextData[{
 "Since our problem is already in the perturbed form, we can use the \
perturbation theory and let \n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", " ", "\[Tilde]", " ", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"n", "=", "0"}], "\[Infinity]"], 
      RowBox[{
       SubscriptBox["x", "n"], 
       SuperscriptBox["\[Epsilon]", "n"]}]}]}], ",", "   ", 
    RowBox[{"\[Epsilon]", "\[Rule]", 
     SuperscriptBox["0", "+"]}]}], TraditionalForm]],
  FontSize->16,ExpressionUUID->"bdf4744f-3ef4-4940-883e-528bdb75d90a"],
 "\n\nNow we replace this in the original problem, and seek the roots using \
this expansion:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"n", "=", "0"}], "\[Infinity]"], 
        RowBox[{
         SubscriptBox["x", "n"], 
         SuperscriptBox["\[Epsilon]", "n"]}]}], ")"}], "2"], "-", 
     RowBox[{"(", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"n", "=", "0"}], "\[Infinity]"], 
       RowBox[{
        SubscriptBox["x", "n"], 
        SuperscriptBox["\[Epsilon]", "n"]}]}], ")"}], " ", "+", " ", 
     RowBox[{"6", "\[Epsilon]"}]}], " ", "=", " ", "0"}], TraditionalForm]],
  FontSize->16,ExpressionUUID->"e95c4ed7-8240-45f8-b210-28d9e56ea920"],
 StyleBox[".\n\n",
  FontSize->16],
 "so now we use Mathematica to solve this: "
}], "Text",
 CellChangeTimes->{{3.826844668730858*^9, 3.826844726271653*^9}, {
  3.826844789369735*^9, 
  3.8268449217959347`*^9}},ExpressionUUID->"f4984504-155c-4c29-8261-\
b4dbd1e04edf"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"SeriesCoefficient", "[", 
  RowBox[{
   RowBox[{"Series", "[", 
    RowBox[{
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"Sum", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"x", "[", "n", "]"}], 
           SuperscriptBox["\[Epsilon]", "n"]}], ",", 
          RowBox[{"{", 
           RowBox[{"n", ",", "0", ",", "10"}], "}"}]}], "]"}], ")"}], "2"], 
      "-", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"x", "[", "n", "]"}], 
         SuperscriptBox["\[Epsilon]", "n"]}], ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", "0", ",", "10"}], "}"}]}], "]"}], "+", 
      RowBox[{"6", "\[Epsilon]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Epsilon]", ",", "0", ",", "2"}], "}"}]}], "]"}], ",", "0"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.826844931719924*^9, 3.826844951980798*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"392142c9-a613-404b-9a87-6989d5382de3"],

Cell[BoxData[
 RowBox[{
  RowBox[{"-", 
   RowBox[{"x", "[", "0", "]"}]}], "+", 
  SuperscriptBox[
   RowBox[{"x", "[", "0", "]"}], "2"]}]], "Output",
