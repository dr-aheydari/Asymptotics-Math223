Cell[TextData[{
 "so that means that we have:\n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["x", "TI"], "1"], "\[LongEqual]", "0", "+", "6", 
